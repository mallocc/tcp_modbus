    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Slave",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/slave/slave",
            "cwd": "${workspaceFolder}/slave"
        },
        {
            "name": "Debug Master",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/master/master",
            "cwd": "${workspaceFolder}/master",
            "miDebuggerPath": "${workspaceFolder}/gdb.sh"
        }
    ]
}